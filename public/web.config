<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <system.webServer>
    <defaultDocument>
      <files>
        <clear />
        <add value="index.php" />
      </files>
    </defaultDocument>
        <staticContent>
            <mimeMap fileExtension=".ipa" mimeType="application/octet-stream" />
        </staticContent>
        <handlers>
            <add name="PHP_via_FastCGI" path="*.php" verb="*" modules="FastCgiModule" scriptProcessor="C:\tools\php83\php-cgi.exe" resourceType="File" requireAccess="Script" />
        </handlers>
  </system.webServer>
</configuration>